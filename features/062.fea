
lookup caltContextualAlternateslookup62 {
  lookupflag 0;
    ignore sub [\bar ] [\bar ]' [\bar ] ;
    ignore sub [\bar ]' [\bar ] [\bar ] ;
    ignore sub [\hyphen \equal ] [\bar ]' [\bar ] ;
    ignore sub [\bar ]' [\bar ] [\hyphen ] ;
    ignore sub [\bar ]' [\bar ] [\equal ] [\colon \exclam \slash \bar \equal \greater \less ] ;
    sub [\bar.spacer ] [\bar ]'  by [\bar_bar.liga ];
    sub [\bar ]' [\bar ]  by [\bar.spacer ];
} caltContextualAlternateslookup62;

